{
  "name": "neoflow/github-api-client",
  "description": "GitHub API Client",
  "type": "library",
  "license": "MIT",
  "version": "1.0.0-beta",
  "minimum-stability": "stable",
  "autoload": {
    "classmap": [
      "src/classes"
    ],
    "files": []
  },
  "autoload-dev": {
    "classmap": [
      "tests/classes"],
    "files": []
  },
  "scripts": {
    "test": "phpunit",
    "php-cs-fixer": "php-cs-fixer fix",
    "php-cs-fixer dry-run": "php-cs-fixer fix --dry-run"
  },
  "config": {
    "process-timeout": 0
  },
  "require": {
    "ext-curl": "*",
    "ext-json": "*"
  },
  "require-dev": {
    "friendsofphp/php-cs-fixer": "^2.16",
    "phpunit/phpunit": "^9.2"
  }
}
